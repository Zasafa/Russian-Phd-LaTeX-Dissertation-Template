@article{Mann-2011, 
author = {Sander A. Mann and Richard R. Grote and Richard M. Osgood and Jon A. Schuller}, 
journal = {Opt. Express}, 
keywords = {Diffraction gratings; Optical resonators; Mie theory; Solar energy; Subwavelength structures ; Thin film devices and applications},
number = {25}, 
pages = {25729--25740}, 
publisher = {OSA},
title = {Dielectric particle and void resonators for thin film solar cell textures}, 
volume = {19}, 
month = {Dec},
year = {2011},
url = {http://www.opticsexpress.org/abstract.cfm?URI=oe-19-25-25729},
doi = {10.1364/OE.19.025729},
abstract = {Abstract: Using Mie theory and Rigorous Coupled Wave Analysis (RCWA) we compare the properties of dielectric particle and void resonators. We show that void resonators---low refractive index inclusions within a high index embedding medium---exhibit larger bandwidth resonances, reduced peak scattering intensity, different polarization anisotropies, and enhanced forward scattering when compared to their particle (high index inclusions in a low index medium) counterparts. We evaluate amorphous silicon solar cell textures comprising either arrays of voids or particles. Both designs support substantial absorption enhancements (up to 45\%) relative to a flat cell with anti-reflection coating, over a large range of cell thicknesses. By leveraging void-based textures 90\% of above-bandgap photons are absorbed in cells with maximal vertical dimension of 100 nm.},
}


